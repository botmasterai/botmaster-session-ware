      "botmaster": ">3.0.0"
  },
  "homepage": "https://github.com/botmasterai/botmaster-mem-session-ware#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2015": "^6.22.0",
    "documentation": "^4.0.0-beta.18",
    "coveralls": "^2.11.15",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.2.0",
    "should": "^11.2.0"
